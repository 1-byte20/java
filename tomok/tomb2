import java.util.Scanner;


public class Tomb2 {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.println("Kérem adja meg az adatok számot: ");
        int t = sc.nextInt();

        double[] gyuszi =new double[t];

        for (int i =0; i<t; i++){
            System.out.println("Kérem adjon meg egy számot: ");
            gyuszi[i]=sc.nextDouble();
        }
        for (int i =0; i<t; i++ ){
            System.out.println(i+1+". : "+ gyuszi[i]);
        }

    }
}
